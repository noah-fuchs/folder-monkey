{
  "manifest_version": 3,
  "name": "FolderMonkey",
  "version": "1.0",
  "description": "Inject modular JavaScript into any website. Organize scripts in folders with ES module support.",
  "action": {
    "default_popup": "popup/index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}