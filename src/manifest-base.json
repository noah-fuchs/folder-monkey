{
  "manifest_version": 3,
  "name": "Script Executor",
  "version": "1.0",
  "description": "Execute modular user scripts on specified domains.",
  "action": {
    "default_popup": "popup/index.html"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}
